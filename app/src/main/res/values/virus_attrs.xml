<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Virus">
        <attr name="virusSize" format="dimension"/>
        <attr name="virusColor" format="color"/>
        <attr name="virusShape" format="enum">
            <enum name="square" value="0"/>
            <enum name="oval" value="1"/>
            <enum name="triangle" value="2"/>
        </attr>
    </declare-styleable>
</resources>